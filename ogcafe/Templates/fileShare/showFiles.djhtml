{% extends "fileShare/main.djhtml" %}
{% block title %} List of newest uploaded files {% endblock %}

{% block content %}
	<b>Last 10 uploaded public files </b><br>
	{% if top_newest_files%}
	{% for data in top_newest_files %}
        <a href="/files/FileShare/{{ data.fileObject }}">{{ data.name }}</a> by {{ data.owner}} ({{ data.timeOfUpload }})<br>
	{% endfor %}
	{% else %}
		No PUBLIC files uploaded yet!
	{% endif %}
{% endblock %}