{% extends "grades/main.djhtml" %}

{% block title %} Subjects{% endblock %}

{% block content %}

<h2>Subjects:</h2>

<table>
<tr><td><b>Subject:</b></td><td><b>Teacher:</b> </td></tr>  
{% for subject in subjects %}
	<tr><td><a href="{% url subject subjectID=subject.id %}">{{subject}}</a></td><td>{{ subject.teacher }} </td></tr>  
{% endfor %}
</table>


{% endblock %}
